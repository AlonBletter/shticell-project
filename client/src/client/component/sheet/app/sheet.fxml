<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>

<ScrollPane fx:id="mainScrollPane" fitToHeight="true" fitToWidth="true" stylesheets="@sheet.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.component.sheet.app.SheetController">
   <content>
      <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="798.0" prefWidth="928.0">
         <children>
            <BorderPane fx:id="borderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="798.0" minWidth="928.0" prefHeight="798.0" prefWidth="928.0">
                <top>
                    <fx:include fx:id="headerComponent" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" source="../header/header.fxml" />
                </top>
               <left>
                  <fx:include fx:id="leftComponent" maxHeight="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" source="../left/left.fxml" BorderPane.alignment="TOP_CENTER" />
               </left>
            </BorderPane>
            <Button fx:id="backButton" alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#backButtonOnClicked" prefHeight="50.0" prefWidth="100.0" styleClass="back-button" text="Back to Dashboard" textAlignment="CENTER" wrapText="true" StackPane.alignment="BOTTOM_RIGHT">
               <StackPane.margin>
                  <Insets bottom="25.0" right="25.0" />
               </StackPane.margin>
            </Button>
         </children>
      </StackPane>
   </content>
</ScrollPane>
